Create Table User (
    Id                  int Unique,
    Name                varchar(50) Not Null,
    Date_of_Birth       varchar(50),
    Email               varchar(50) Not Null,
    Primary Key (Id)
);

Create Table Account (
    User_id             int Not Null AUTO_INCREMENT,
    Account_Number      int AUTO_INCREMENT,
    Account_Type        varchar(50) Not Null,
    Primary Key (Account_Number)
);

Create Table Business_Account (
    Account_Number      int AUTO_INCREMENT,
    Company_Name        varchar(50) Not Null,
    Monthly_Fee         decimal(4,2),
    Primary Key (Account_Number)
    Foreign Key (Account_Number) References Account(Account_Number)
);

Create Table Personal_Account (
    Account_Number      int AUTO_INCREMENT,
    Id                  int Unique,
    Name                varchar(50) Not Null,
    Date_of_Birth       varchar(50),
    Email               varchar(50) Not Null,
    Primary Key (Account_Number)
    Foreign Key (Account_Number) References Account(Account_Number)
);

Create Table Page (
    Page_id             int AUTO_INCREMENT,
    Account_Number      int Not Null AUTO_INCREMENT,
    Page_Name           varchar(50) Unique,
    Number_of_Visits    int,
    Primary Key (Page_id)
    Foreign Key (Account_Number) References Account(Account_Number)
);

Create Table Advertisements_Page (
    Page_id             int AUTO_INCREMENT,
    Bounce_Rate         decimal(3,2),
    Click_Through_Rate  decimal(3,2),
    Conversion_Rate     decimal(3,2),
    Primary Key (Page_id)
    Foreign Key (Page_id) References Page(Page_id)
);

