Create Table User (
    Id                  int,
    Name                char(50) Not Null,
    Date_of_Birth       varchar(50),
    Email               varchar(50) Not Null,
    Primary Key (Id)
);

Create Table Account (
    Account_Number      int,
    Account_Type        varchar(50) Not Null,
    Primary Key(Account Number)
    Foreign Key(Account_Number) References User(Id)
);

Create Table Business Account (
    Company_Name        varchar(50),
    Monthly_Fee         decimal(4,2)
);

Create Table Personal Account (
    Id                  int Unique,
    Name                varchar(50) Not Null,
    Date_of_Birth       varchar(50),
    Email               varchar(50) Not Null
);

Create Table Advertisements Page (
    Bounce_Rate         decimal(3,2),
    Click_Through_Rate  decimal(3,2),
    Conversion_Rate     decimal(3,2)
);

Create Table Page (
    Page_Name           varchar(50) Unique,
    Number_of_Visits    int
);
